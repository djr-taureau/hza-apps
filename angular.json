      "root": "apps/doc-repo",
      "sourceRoot": "apps/doc-repo/src",
      "prefix": "hza",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/doc-repo",
