{
  "name": "hza",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "ng serve --aot",
    "server": "json-server ./db.json --watch",
    "dev-with-server": "concurrently \"npm run server\" \"npm run start\"",
    "upload-server": "npm run ./server.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "clean": "rimraf dist",
    "sand": "ng serve sandbox-site",
    "g:migration": "yarn typeorm migration:generate -n",
    "ms:build:clean": "rimraf ./dist/microservices",
    "ms:build:ts": "tsc -p microservices/tsconfig.microservices.json",
    "ms:build:copy": "copyfiles -u 1 \"microservices/**/*\" -e \"**/*(*.ts|*.microservices.json)\" dist/microservices",
    "ms:build": "npm run ms:build:clean && npm run ms:build:ts && npm run ms:build:copy",
    "ms:pack": "funcpack pack -c ./dist/microservices -o ../../dist ",
    "ms:run": "cd ./dist/microservices; func start",
    "sql:build:image": "docker build ./tools/docker/sql-server --tag hza/sql-server:latest",
    "sql:build:container": "docker run -it  -p 1433:1433 --name hza-sql -d hza/sql-server:latest",
    "sql:stop": "docker stop hza-sql",
    "sql:start": "docker start hza-sql",
    "sql:remove": "docker rm hza-sql",
    "sql:build": "npm run sql:build:image && npm run sql:build:container",
    "sql:update": "npm run sql:build:image && npm run sql:stop && npm run sql:remove && npm run sql:build:container",
    "setup:ssl": "./tools/scripts/gen-ssl.sh",
    "setup:sql": "npm run sql:build",
    "env:echo": "node ./tools/scripts/env-echo.js",
    "db:seed": "./node_modules/.bin/ts-node ./tools/scripts/db-seed.ts",
    "db:sync": "./tools/scripts/db-sync.sh",
    "db:gen:migrations": "./tools/scripts/migrations-generate.sh",
    "db:gen": "./tools/scripts/migrations-run.sh",
    "db:gen:revert": "./tools/scripts/migrations-revert.sh",
    "db:config:orm": "node ./tools/scripts/orm-config.js",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && node ./decorate-angular-cli.js"
  },
  "private": true,
